{
  "name": "template",
  "version": "1.0.0",
  "description": "be-template",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --trace-warnings --verbose src/server.js",
    "start": "node src/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint --format codeframe .",
    "prettier:check": "prettier --config .prettierrc.json --check .",
    "prettier:fix": "prettier --config .prettierrc.json --write .",
    "prepare": "git init && husky install",
    "clean:cache": "rimraf node_modules/.cache",
    "clean:lib": "rimraf node_modules package-lock.json",
    "reinstall": "npm run clean:lib && npm install"
  },
  "author": "thang",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "argon2": "^0.40.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "googleapis": "^134.0.0",
    "husky": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.7.0",
    "mongoose": "^8.2.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.2",
    "nodemailer": "^6.9.12",
    "nodemo": "^1.0.0",
    "nodemon": "^3.1.0",
    "pino": "^8.19.0",
    "sequelize": "^6.37.1",
    "socket.io": "^4.7.5"
  }
}
